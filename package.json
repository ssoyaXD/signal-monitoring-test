{
  "name": "signal-monitoring-test",
  "version": "1.0.0",
  "description": "Monitoring system with NestJS, Prometheus, and Grafana",
  "private": true,
  "scripts": {
    "install:all": "pnpm install",
    "dev": "pnpm --filter backend start:dev",
    "dev:backend": "pnpm --filter backend start:dev",
    "build": "pnpm --filter backend build",
    "build:backend": "pnpm --filter backend build",
    "start": "pnpm --filter backend start:prod",
    "start:backend": "pnpm --filter backend start:prod",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:rebuild": "docker-compose up -d --build",
    "stop": "./stop.sh",
    "cleanup": "./cleanup-ports.sh",
    "clean": "pnpm --filter backend clean"
  },
  "keywords": [
    "nestjs",
    "prometheus",
    "grafana",
    "monitoring",
    "pnpm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}


      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PostgreSQL",
            "name": "PostgreSQL"
          },
          "rawQuery": true,
          "rawSql": "SELECT DATE(v.timestamp) AS diagnosis_date, s.sensor_name, s.sensor_id, MAX(v.rms_value) AS max_rms, MIN(v.rms_value) AS min_rms, AVG(v.rms_value) AS avg_rms, COUNT(CASE WHEN v.status = 'critical' THEN 1 END) AS critical_count, COUNT(CASE WHEN v.status = 'warning' THEN 1 END) AS warning_count, COUNT(CASE WHEN v.status = 'normal' THEN 1 END) AS normal_count, COUNT(*) AS total_count, CASE WHEN COUNT(CASE WHEN v.status = 'critical' THEN 1 END) > 0 THEN '위험' WHEN COUNT(CASE WHEN v.status = 'warning' THEN 1 END) > 0 THEN '경고' ELSE '정상' END AS daily_diagnosis FROM sensors s JOIN vibration_data v ON s.sensor_id = v.sensor_id WHERE v.timestamp >= NOW() - INTERVAL '90 days' GROUP BY DATE(v.timestamp), s.sensor_name, s.sensor_id ORDER BY diagnosis_date DESC, s.sensor_id",
          "refId": "A",
          "format": "table"
        }
